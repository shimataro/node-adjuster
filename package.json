{
  "name": "value-schema",
  "description": "simple, easy-to-use, and declarative schema validator",
  "version": "2.2.5",
  "author": "shimataro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shimataro/value-schema.git"
  },
  "bugs": {
    "url": "https://github.com/shimataro/value-schema/issues"
  },
  "homepage": "https://github.com/shimataro/value-schema#readme",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "param",
    "params",
    "parameter",
    "parameters",
    "validate",
    "validator",
    "ajust",
    "adjuster",
    "revise",
    "revisor",
    "schema",
    "JSON schema"
  ],
  "main": "./dist/index",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "default": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">=2.0.0"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:1-esm": "run-s build:1-esm:*",
    "build:1-esm:1-tsc": "tsc -p tsconfig.esm.json",
    "build:1-esm:2-babel": "babel ./dist --out-dir ./dist --out-file-extension .mjs",
    "build:2-cjs": "tsc -p tsconfig.cjs.json",
    "test": "jest --config jest.config.json",
    "verify": "run-p test lint",
    "lint": "run-p lint:*",
    "lint:ts": "eslint ./src ./test --ext .ts",
    "lint:md": "markdownlint . --ignore ./node_modules",
    "lint:yaml": "yamllint .*.yml *.yml --ignore=node_modules/**/*.yml --ignore=node_modules/**/*.yaml",
    "check-updates": "ncu"
  },
  "dependencies": {
    "core-js": "3.6.4"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "babel-plugin-module-extension-resolver": "0.3.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "markdownlint-cli": "0.21.0",
    "npm-check-updates": "4.0.1",
    "npm-run-all": "4.1.5",
    "ts-jest": "25.2.0",
    "typescript": "3.7.5",
    "yaml-lint": "1.2.4"
  }
}
