{
  "comments": false,
  "presets": [
    ["@babel/preset-env", {"targets": {"node": "current"}, "useBuiltIns": "usage"}]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
    ["add-module-exports"]
  ],
  "env": {
    "esm": {
      "presets": [
        ["@babel/preset-env", {"targets": {"node": "current"}, "useBuiltIns": "usage", "modules": false}]
      ]
    }
  }
}
